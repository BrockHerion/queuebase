{
	"name": "@queuebase/monorepo",
	"type": "module",
	"scripts": {
		"build:all": "turbo run build",
		"build": "turbo run build --filter \"./packages/*\"",
		"dev": "turbo run dev --filter \"./packages/*\"",
		"publish": "turbo run publish --filter \"./packages/queuebase\""
	},
	"workspaces": [
		"examples/*",
		"packages/*",
		"packages/config/*"
	],
	"dependencies": {
		"@types/node": "^20.0.0",
		"eslint": "^9.1.1",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"turbo": "^1.13.2",
		"typescript": "^5.4.5"
	}
}